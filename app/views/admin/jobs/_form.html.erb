<%= simple_form_for [:admin, @job ] do |f| %>

<%= f.input :title , :label => "目的地"%>
<%= f.input :description , :label => "行程简介"%>
<%= f.input :wage_lower_bound, :label => "人数下限" %>
<%= f.input :wage_upper_bound, :label => "人数上限" %>
<%= f.input :contact_email , :label => "出发日期"%>
<%= f.input :is_hidden %>

  <%= f.submit "Submit",:label => "发布" %>
<% end %>
